#include <stdio.h>

int countPairs(int nums[], int n, int k) {
    int i, j, count = 0;
    for (i = 0; i < n; i++) {
        for (j = i + 1; j < n; j++) {
            if (nums[i] == nums[j] && (i * j) % k == 0) {
                count++;
            }
        }
    }
    return count;
}

int main() {
    int i, n, k;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int nums[n];
    printf("Enter elements:\n");
    for (i = 0; i < n; i++)
        scanf("%d", &nums[i]);

    printf("Enter value of k: ");
    scanf("%d", &k);

    int result = countPairs(nums, n, k);
    printf("Output: %d\n", result);

    return 0;
}
