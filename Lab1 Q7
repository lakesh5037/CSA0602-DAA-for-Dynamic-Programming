#include <stdio.h>
#include <stdbool.h>

bool isPresent(int arr[], int size, int key) {
	int i;
    for (i = 0; i < size; i++) {
        if (arr[i] == key)
            return true;
    }
    return false;
}

int main() {
    int i, n;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int input[n];
    printf("Enter elements:\n");
    for (i = 0; i < n; i++)
        scanf("%d", &input[i]);

    int unique[n];
    int uCount = 0;

    for (i = 0; i < n; i++) {
        if (!isPresent(unique, uCount, input[i])) {
            unique[uCount++] = input[i];
        }
    }

    printf("Unique elements: ");
    for (i = 0; i < uCount; i++)
        printf("%d ", unique[i]);
    printf("\n");

    return 0;
}
