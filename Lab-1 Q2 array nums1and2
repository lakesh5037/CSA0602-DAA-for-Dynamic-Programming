#include <stdio.h>
#include <stdbool.h>

bool existsInArray(int arr[], int size, int key) {
	int i;
    for (i = 0; i < size; i++) {
        if (arr[i] == key)
            return true;
    }
    return false;
}

int main() {
    int i, n, m;

    printf("Enter size of nums1: ");
    scanf("%d", &n);
    int nums1[n];
    printf("Enter elements of nums1:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &nums1[i]);
    }

    printf("Enter size of nums2: ");
    scanf("%d", &m);
    int nums2[m];
    printf("Enter elements of nums2:\n");
    for (i = 0; i < m; i++) {
        scanf("%d", &nums2[i]);
    }

    int answer1 = 0;
    for (i = 0; i < n; i++) {
        if (existsInArray(nums2, m, nums1[i])) {
            answer1++;
        }
    }

    int answer2 = 0;
    for (i = 0; i < m; i++) {
        if (existsInArray(nums1, n, nums2[i])) {
            answer2++;
        }
    }

    printf("Output: [%d,%d]\n", answer1, answer2);

    return 0;
}
