#include <stdio.h>

void selectionSort(int arr[], int n) {
    int i, j, minIdx, temp;
    for (i = 0; i < n - 1; i++) {
        minIdx = i;
        for (j = i + 1; j < n; j++) {
            if (arr[j] < arr[minIdx])
                minIdx = j;
        }
        temp = arr[i];
        arr[i] = arr[minIdx];
        arr[minIdx] = temp;
    }
}

int main() {
    int n;
    printf("Enter size of array: ");
    scanf("%d", &n);

    if (n == 0) {
        printf("Output: [] (Empty list)\n");
        return 0;
    }

    int i, arr[n];
    printf("Enter elements:\n");
    for (i = 0; i < n; i++);
        scanf("%d", &arr[i]);

    if (n == 1) {
        printf("Output: [%d]\n", arr[0]);
        return 0;
    }
    selectionSort(arr, n);    
    printf("Output: [");
    for (i = 0; i < n; i++) {
        printf("%d", arr[i]);
        if (i != n - 1)
            printf(", ");
    }
    printf("]\n");

    return 0;
}
