#include <stdio.h>
#include <stdbool.h>

int countDistinct(int arr[], int start, int end) {
    int freq[1001] = {0}; 
    int i, count = 0;
    for (i = start; i <= end; i++) {
        if (freq[arr[i]] == 0)
            count++;
        freq[arr[i]]++;
    }
    return count;
}

int main() {
    int i, j, n;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int nums[n];
    printf("Enter elements:\n");
    for (i = 0; i < n; i++)
        scanf("%d", &nums[i]);

    int total = 0;
    for (i = 0; i < n; i++) {
        for (j = i; j < n; j++) {
            int distinct = countDistinct(nums, i, j);
            total += distinct * distinct;
        }
    }

    printf("Output: %d\n", total);
    return 0;
}
